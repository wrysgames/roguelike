{
	"name": "rogue-realms",
	"globIgnorePaths": ["**/package.json", "**/tsconfig.json"],
	"tree": {
		"$className": "DataModel",
		"ServerScriptService": {
			"$className": "ServerScriptService",
			"server": {
				"$path": "out/server"
			}
		},
		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"rbxts_include": {
				"$path": "include",
				"node_modules": {
					"$className": "Folder",
					"@rbxts": {
						"$path": "node_modules/@rbxts"
					},
					"@flamework": {
						"$path": "node_modules/@flamework"
					}
				}
			},
			"shared": {
				"$path": "out/shared"
			},
			"weapons": {
				"$path": "assets/weapons"
			},
			"vfx": {
				"$path": "assets/vfx"
			}
		},
		"StarterPlayer": {
			"$className": "StarterPlayer",
			"StarterPlayerScripts": {
				"$className": "StarterPlayerScripts",
				"client": {
					"$path": "out/client"
				}
			}
		},
		"HttpService": {
			"$className": "HttpService",
			"$properties": {
				"HttpEnabled": true
			}
		},
		"SoundService": {
			"$className": "SoundService",
			"$properties": {
				"RespectFilteringEnabled": true
			}
		},
		"Lighting": {
			"$properties": {
				"Ambient": [0, 0, 0],
				"Brightness": 2,
				"GlobalShadows": true,
				"Outlines": false,
				"Technology": "Voxel"
			}
		}
	}
}
